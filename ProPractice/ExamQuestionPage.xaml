<?xml version="1.0" encoding="utf-8"?>
<Page
    x:Class="ProPractice.ExamQuestionPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <StackPanel Orientation="Vertical">
        <TextBlock x:Name="QuestionTextTextBlock" Style="{StaticResource SubtitleTextBlockStyle}" Margin="0,0,0,10"/>
        <TextBlock x:Name="AnswerTypeTextBlock"/>
        <StackPanel x:Name="ContentStackPanel" Orientation="Vertical" HorizontalAlignment="Left"/>
    </StackPanel>
</Page>
